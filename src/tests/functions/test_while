    int i = 4;
    while (i < 10) {
        i++;
    }

    double j = 4.004;
    while (j < 10) {
        j++;
    }

    while (i < 10) {
        while (j < 10) {
            i++;
            j++;
        }
    }

    while (i < 10) {
        while (j < 10) {
            while (i < 10) {
                i++;
                j++;
            }
        }
    }