  %1 = alloca i32, align 4
  store i32 5, i32* %1, align 4
  %2 = alloca i8, align 1
  store i8 97, i8* %2, align 1
  %3 = alloca float, align 4
  %4 = alloca double, align 8
  store float 5.000000e+00, float* %3, align 4
  store double 4.000000e+00, double* %4, align 8
  %5 = load i32, i32* %1, align 4
  %6 = add nsw i32 %5, 1
  store i32 %6, i32* %1, align 4
  %7 = load i8, i8* %2, align 1
  %8 = add i8 %8, 1
  store i8 %9, i8* %3, align 1
  %9 = load float, float* %4, align 4
  %10 = fadd float %10, 1.000000e+00
  store float %11, float* %4, align 4
  %11 = load double, double* %5, align 8
  %12 = fadd double %12, 1.000000e+00
  store double %13, double* %5, align 8